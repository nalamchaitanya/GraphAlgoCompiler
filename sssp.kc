function main(int argc)
{
	readGraph();
	Node u,v,w;
	int contn,wt,i,j;
	foreach node u : i
	{
		u.dist=999999999;
	}
	w=getNode(0);
	w.dist=0;
	contn=1;
	while(contn==1)
	{
		contn=0;
		foreach node u : i
		{
			foreach edge u to v : j
			{
				wt=weight(u,v);
				if(u.dist+wt<v.dist)
				{
					v.dist=u.dist+wt;
					contn=1;
				}
			}
		}
	}
	foreach node u : i
	{
		printLn u.dist;
	}
}
