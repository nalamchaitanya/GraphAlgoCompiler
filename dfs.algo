#dfs implementation
@#include <stdio.h>
@#include "Graph.c"
@Graph *graph;
@

#DFS function.
function DFS(Node u,int num)
{
	Node v;
	int i;
	u.dfsnum = num;
	printLn u.name;
	# print "String has to be printed.";
	foreach edge u to v : i
	{
		if(v.dfsnum==0)
		{
			num=num+1;
			DFS(v,num);
		}
	}
}

#main function.
function main(int argc)
{
	Node u;
	int i;
	readGraph();
	foreach node u : i
	{
		u.dfsnum = 0;
	}
	u=getNode(0);
	DFS(u,1);
}
