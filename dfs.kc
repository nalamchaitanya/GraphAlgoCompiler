#dfs implementation

#DFS function.
function DFS(Node u,int num)
{
	Node u,v;
	int i;
	u.dfsnum = num;
	printLn u.dfsnum;
	print "String has to be printed.";
	foreach edge u to v : i
	{
		if(v.dfsnum==0)
		{
			num=num+1;
			DFS(v,num);
		}
	}
}

#main function.
function main(int argc)
{
	Node u;
	int i;
	foreach node u : i
	{
		u.dfsnum = 0;
	}
	DFS(0,1);
}
